{
"errorpages.path":"/ca/",
"realmName":"Caimito",
"datasource.name":"caimito",
"db.source":"datasource",
"deployment.mode":"development",
"view.extension":"index.jamun",
"#view.engine.class":"org.jamun.views.JQueryUIView",
"view.engine.class":"org.jamun.views.ZurbFoundationView",
"view.engine.template":"default",
"production.jquery.js.bundle":["jquery","jquery-ui"],
"production.jamun.js.bundle":["jamun-util","jamun","jamun-ui"],
"mobile.clients":["Apple iOS","Android","Windows Phone","Blackberry","Palm WebOS","Firebox Mobile","Chrome for Android","Skyfire","Opera Mobile","Meego","Samsung bada","UC Browser","Kindle","Nook"],
"mobile.redirect":"jamun-mobile",
"disable.jsp":true,
"jamunapps.table":"ja_jamunapps",
"mailSession":"java:comp/env/mail/MailSession",
"sendmail.roles":"jamunadmin",
"implicit.functions":"RandomAlphNumeric",
"RandomAlphNumeric.function":"tools.util.AlphaNumeric.generateRandomAlphNumeric",
"implicit.serverobjs":"captcha.ca,sendmail.sm",
"ssb.serverobj.class":"org.jamun.server.objs.SSBatchMgrServerObjs",
"ssi.serverobj.class":"org.jamun.server.objs.SSIBatchMgrServerObjs",
"ca.serverobj.class":"org.jamun.server.objs.impl.CaptchaServerObjs",
"sm.serverobj.class":"org.jamun.server.objs.impl.SendmailServerObjs",
"rep.serverobj.class":"org.jamun.server.objs.impl.RESTProxyServerObjs",
"local.apps":["ja"],
"#resource.mgr":"network",
"resource.mgr":"file",
"shared.network.dir":"/netdrive",
"network.ResourceMgr.class":"org.jamun.server.resourcemgr.SharedNetworkFileMgr",
"file.ResourceMgr.class":"org.jamun.server.resourcemgr.FileMgr",
"#minutes":"",
"request.timeout":"5",
"max_batch_size":"200",
"anonymous.role":"guest",
"anonymous.user":"guest",
"deployment.mode":"development",
"results.page.max.size":"500",
"disallowednames":"jamun*,results.page,results.pages,access,usermgr,owner,all,realmmgr,member,guest,admin",
"jamunuser.create":"all",
"defaultrealm":"member",
"defaultrealm.member":"enduser",
"defaultrealm.enduser":"subuser",
"jamunadmin":"<%= @mysqlUsername %>",
"jamunadminpassword":"<%= @mysqlPassword %>",
"jamunadminrealm":"jamunadmin",
"jamunadminrole":"jamunadmin",
"defaultdigest":"PLAIN",
<% @ldapAuth.each do |name, value| %>
<% if name=='enable' %>
<% if value %>
"#defaultusertype":"db",
"defaultusertype":"ldap",
<% else %>
"defaultusertype":"db",
"#defaultusertype":"ldap",
<% end %>
<% else %>
"<%= name %>":"<%= value %>",
<% end %>
<% end %>
"#minutes":"",
"jamunsession.maxage":"30",
"dateformat":"dd/MMM/yyyy:HH:mm:ss:SSS",
"user.can.create.realms":"true",
"defaultrealm.members.can.invite":"true",
"defaultrealm.secret.realm.memberships":"true",
"allow.secret.realm.memberships":"true",
"max.user.realms":"5",
"max.user.realm.invites":"5",
"max.user.realm.members":"25",
"max.user.realm.membership":"25",
"#minutes":"",
"user.realm.invite.maxage":"1440",
"defaultrealmtype":"db",
"max_name_length":"50",
"min_name_length":"3",
"alphanumeric_names_only":"true",
"allowed_characters":"-",
"allow_mix_case":"false"
}
